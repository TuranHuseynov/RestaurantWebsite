@model IEnumerable<PubRestoran.Models.AboutU>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/AdminArea/Views/Shared/_adminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.about_header)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.about_text)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.about_header)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.about_text)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.about_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.about_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.about_id })
        </td>
    </tr>
}

</table>
