@model PubRestoran.ViewModel.Mymodel
<!DOCTYPE html>
<html>
<head>
    <title></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Pub/OwlCarousel2-2.3.4/OwlCarousel2-2.3.4/dist/assets/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="~/Pub/OwlCarousel2-2.3.4/OwlCarousel2-2.3.4/dist/assets/owl.theme.default.min.css">
    <link rel="stylesheet" type="text/css" href="~/Pub/css/style.css">
</head>
<body>


    <section id="modalSection">

        <div class="modal" id="simpleModal">
            <div class="modalContent">
                <div class="modalHeader">
                    <span class="btnspan" id="closeModal">&times;</span>
                    <h2>Reservation</h2>
                </div>
                <div class="modalBody">
                    <div class="menusitem text-center">
                        <h1>Wellcome</h1>
                        <p style="font-size: 11px;">I'm a paragraph. Click here to add your own text and edit me. It’s easy. Just click to the font.</p>

                        <a href="#" id="menulistid">Menusssssss</a><br>
                        <div id="menulistitem">
                            <a href="italian.html">Azerbaijan</a>
                            <a href="italian.html">Chine</a>
                            <a href="italian.html">France</a>
                        </div>
                        <input type="text" name="" placeholder="  Date"><br>
                        <input type="text" name="" placeholder="  Time"><br>
                        <input type="text" name="" placeholder="  Party Size"><br>

                        <button class="btnbook">BOOK</button>
                    </div>
                </div>
                <div class="modalFooter text-center">
                    <p style="font-size: 11px;">Address :500 Terry Francois Street SF , CA  94158 </p>
                </div>

            </div>
        </div>


    </section>
    <div class="clearfix"></div>
    <section id="navbar">
        <nav class="text-center">
            <div class="toggle">
                <div class="menu"><i class="fa fa-bars" aria-hidden="true"></i></div>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-md-3">
                        <p>LOGO</p>
                    </div>
                    <div class="col-md-7">
                        <ul>
                            <li><a href="/Home/Index">Home</a></li>
                            <li><a href="/Home/Contact">Contact</a></li>
                            <li><a href="/Home/Gallery">Gallery</a></li>
                            <li><a href="/Home/About">About</a></li>
                        </ul>

                    </div>
                    <div class="col-md-2 iconmicon">
                        <i class="fa fa-search" aria-hidden="true" id="iconss"></i>
                        <input type="text" id="input" name="" placeholder="Axtar ...">
                    </div>
                </div>
            </div>

        </nav>

    </section>
    <div class="clearfix"></div>
    @RenderBody()
    <div class="clearfix"></div>

    <footer id="footer">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    @foreach (var item in Model._about)
                    {
                        <div class="leftitem text-left">
                        <h2>@item.about_header</h2>
                        <p>@item.about_text</p>
                        <p class="text-right"><a href="/Home/About">&raquo; Read more...</a></p>

                    </div>

                    }
                </div>
                <div class="col-md-3">
                    <div class="centeritem">
                        <h2>Category</h2>
                        <ul>
                            <li><a href="">&raquo;Home</a></li>
                            <li><a href="">&raquo;Sweets</a></li>
                            <li><a href="">&raquo;Contetn</a></li>
                            <li><a href="">&raquo;Another</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-3">
                    @foreach (var item in Model._address)
                    {
                      <div class="rightitem text-right">
                        <h2>Bizimle elaqe</h2>
                        <p>Phone : @item.address_phone </p>
                        <p>Adress :  @item.address_map</p>
                        <p>Work Time : @item.address_time</p>
                    </div>


                    }
                </div>
            </div>
        </div>
    </footer>

    <!-- Script files -->
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.3.1.js"></script>

    <script src="https://code.jquery.com/jquery-3.2.1.js"
            integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE="
            crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"
            integrity="sha256-T0Vest3yCU7pafRw9r+settMBX6JkKN06dqBnpQ8d30="
            crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

    <script type="text/javascript" src="~/Pub/OwlCarousel2-2.3.4/OwlCarousel2-2.3.4/docs/assets/vendors/jquery.min.js"></script>
    <script type="text/javascript" src="~/Pub/OwlCarousel2-2.3.4/OwlCarousel2-2.3.4/dist/owl.carousel.js"></script>
    <script src="~/Pub/js/script.js"></script>
</body>
</html>