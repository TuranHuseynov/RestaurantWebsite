@model PubRestoran.ViewModel.Mymodel
@{
    ViewBag.Title = "Gallery";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    #gallery {
        width: 100%;
        background: gray;
        background: linear-gradient(to bottom, rgba(106, 109, 135,0), rgba(157, 159, 181,1));
    }
</style>

<section id="gallery">
    <div class="container">

        <div class="row">
            @foreach (var item in Model._gallery)
        {
            <div class="col-md-12">
                <img src="~/Uploads/@item.gallery_img" style="width:96%; padding:10px;" />
            </div>


    }
        </div>
    </div>
</section>